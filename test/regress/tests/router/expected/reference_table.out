/* Check execution for returning clause */
TRUNCATE test_ref_rel;
NOTICE: send query to shard(s) : sh1,sh2,sh3,sh4
INSERT INTO test_ref_rel VALUES(1,3),(2,4),(3,-1) RETURNING *;
NOTICE: send query to shard(s) : sh1,sh2,sh3,sh4
 i | j  
---+----
 1 |  3
 2 |  4
 3 | -1
(3 rows)

BEGIN;
-- To not interfere below results
update test_ref_rel
set i = 11
where i <= 3 and j < 0
returning i, j;
